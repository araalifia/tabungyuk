<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/historyFragment"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="128dp"
        android:layout_height="27dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="33dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tabungyuk_3" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="16dp"
        android:text="History"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <ListView
        android:id="@+id/historyTransactionListView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/transactionRedirect" />

    <TextView
        android:id="@+id/noHistoryText"
        android:layout_width="57dp"
        android:layout_height="101dp"
        android:layout_marginTop="16dp"
        android:text="No Transactions Found"
        android:textColor="#888"
        android:textSize="16sp"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <Button
        android:id="@+id/transactionRedirect"
        android:layout_width="46dp"
        android:layout_height="53dp"
        android:layout_marginStart="352dp"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="56dp"
        android:backgroundTint="#FBD44B"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="24dp"
        android:layout_height="22dp"
        android:layout_marginStart="11dp"
        app:layout_constraintBottom_toBottomOf="@+id/transactionRedirect"
        app:layout_constraintStart_toStartOf="@+id/transactionRedirect"
        app:layout_constraintTop_toTopOf="@+id/transactionRedirect"
        app:layout_constraintVertical_bias="0.483"
        app:srcCompat="@drawable/add_button" />
</androidx.constraintlayout.widget.ConstraintLayout>
